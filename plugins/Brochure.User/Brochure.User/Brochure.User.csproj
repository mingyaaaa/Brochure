<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'Default' ">
    <StartAction>Program</StartAction>
    <ExternalConsole>true</ExternalConsole>
  </PropertyGroup>
  <Import Project="../../../build/copy_plugin.targets" />
  <ItemGroup>
	  <PackageReference Include="Brochure.Core.Server" Version="0.0.0.8082" ExcludeAssets="runtime" PrivateAssets="none">
	    <IncludeAssets>compile; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
	  </PackageReference>
	  <PackageReference Include="Brochure.ORM.MySql" Version="0.0.0.8082" />
    <PackageReference Include="Grpc.AspNetCore" Version="2.41.0" ExcludeAssets="runtime" PrivateAssets="false" />
    <PackageReference Include="IGeekFan.AspNetCore.Knife4jUI" Version="0.0.11" ExcludeAssets="runtime" PrivateAssets="false" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.2.5" ExcludeAssets="runtime" PrivateAssets="false" />
    <PackageReference Include="Swashbuckle.AspNetCore.Swagger" Version="6.2.3" ExcludeAssets="runtime" PrivateAssets="false" />
    <PackageReference Include="Swashbuckle.AspNetCore.SwaggerGen" Version="6.2.3" ExcludeAssets="runtime" PrivateAssets="false" />
  </ItemGroup>
  <ItemGroup>
    <None Update="plugin.config">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup>
    <Content Include="plugin.config">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="pluginSetting.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Brochure.User.Abstract\Brochure.User.Abstract.csproj" />
  </ItemGroup>
  <ProjectExtensions>
    <VisualStudio>
      <UserProperties />
    </VisualStudio>
  </ProjectExtensions>
</Project>
